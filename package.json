{
  "name": "@enedis-oss/mocha-xray-reporter",
  "version": "1.0.9",
  "description": "Reporter designed to create a xray report according to Jira references in mocha tests",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enedis-OSS/mocha-xray-reporter.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "preferGlobal": true,
  "bin": {
    "mocha-xray-reporter": "./index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose --runInBand",
    "pretest": "npm run build"
  },
  "keywords": [
    "mocha",
    "xray",
    "test",
    "reporter",
    "cypress"
  ],
  "author": "Alain Begey <bluepioupiou@gmail.fr>",
  "license": "MIT",
  "dependencies": {
    "chalk": "5.2.0",
    "commander": "10.0.1",
    "dotenv": "16.1.4",
    "json-diff": "1.0.6",
    "mocha": "10.2.0",
    "ts-typed": "1.3.6"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/core-js": "^2.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
